{
  "name": "Trello Focus",
  "description": "Bug fixes & better display of comments",
  "version": "0.8",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://trello.com/*"],
      "css": ["css/styles.css"],
      "js": [
      "js/libs/jquery-2.1.4.min.js",
      "js/libs/jquery.highlight.js",
      "js/app.js"]
    }
  ],
  "icons": { "16": "icon16.png",
         "48": "icon48.png",
        "128": "icon128.png" },
  "manifest_version": 2
}