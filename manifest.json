{
  "name": "My Trello",
  "description": "Add up features to Trello",
  "version": "0.1",
  // "permissions": [
  //   "tabs", "http://*/*", "https://*/*"
  // ],
  "browser_action": {
      "default_title": "Trello extension",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://trello.com/*"],
      "css": ["css/styles.css"],
      "js": [
      "js/libs/jquery-2.1.4.min.js",
      "js/libs/jquery.highlight.js",
      "js/libs/trello-client.js",
      "js/app.js"]
    }
  ],
  "manifest_version": 2
}